  text-halo-color: #a0a0a0;
  set .landuse_text;
}

area.landuse|z5-::landuse {
  layer: -10;
  set area=eval(area(prop("geo")));
  z-index: eval(-1*tag("area"));
}
area.landuse|z14-::landuse {
  color: eval(prop("fill-color"));
  width: 1;
  linecap: round;
  linejoin: round;
}

area|z5-[leisure=nature_reserve]::landuse
{
  color: #7acc49;

  // make sure the following eval statement works
  width: 1;
  width: 2;
  width: 4;

  width: eval(cond(zoom()>=14, 4, cond(zoom()>=12, 2, 1)));
}

area.landuse_text|z5-[area>1500][area<=6000]::landuse_label {
  text-halo-radius: 1;
  font-size: 10;
  text-wrap-width: 20;
  font-style: italic;
  text: eval(cond(tag('name:en'), tag('name') . '\n' . tag('name:en'), tag('name')));
  text-halo-color: eval(prop("text-halo-color", default));
  text-color: eval(prop("text-color", default));
  layer: 10;
}
area.landuse_text|z5-[area>6000][area<=500000]::landuse_label {
  text-halo-radius: 1;
  font-size: 12;
  font-style: italic;
  text-wrap-width: 20;
  text: eval(cond(tag('name:en'), tag('name') . '\n' . tag('name:en'), tag('name')));
  text-halo-color: eval(prop("text-halo-color", default));
  text-color: eval(prop("text-color", default));
  layer: 10;
}
