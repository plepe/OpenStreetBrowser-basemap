/* LANDUSE */
area|z5-[waterway=dock]::landuse,
area|z5-[landuse=water]::landuse,
area|z5-[landuse=reservoir]::landuse,
area|z5-[landuse=lake]::landuse,
area|z5-[natural=water]::landuse,
area|z5-[landuse=basin]::landuse
{
  fill-color: #7eb9e3;
  set .landuse;

  set .landuse_text;
  text-color: #156299;
  text-halo-color: #7eb9e3;
}
area|z5-[waterway=riverbank]::landuse {
  fill-color: #7eb9e3;
  set .landuse;
}

area|z5-[natural=land]::landuse
{
  fill-color: #f2efd9;
